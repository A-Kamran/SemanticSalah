    <!-- http://semanticsalah.com/ontology/hasWindowEndTime -->

    <owl:DatatypeProperty rdf:about="http://semanticsalah.com/ontology/hasWindowEndTime">
        <rdfs:domain rdf:resource="http://semanticsalah.com/ontology/SalahTimeWindow"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Every Prayer has a prescribed window of time. This window end time indicates the time after which the Prayer observed will be considered late</rdfs:comment>
        <rdfs:label>Has Window End Time</rdfs:label>
        <owl:equivalentProperty>http://dbpedia.org/property/ends</owl:equivalentProperty>
        <owl:equivalentProperty>https://schema.org/endTime</owl:equivalentProperty>
    </owl:DatatypeProperty>
    


    <!-- http://semanticsalah.com/ontology/hasWindowStartTime -->

    <owl:DatatypeProperty rdf:about="http://semanticsalah.com/ontology/hasWindowStartTime">
        <rdfs:domain rdf:resource="http://semanticsalah.com/ontology/SalahTimeWindow"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Every Prayer has a prescribed window of time. This window start time indicates the time after which the Prayer can be observed</rdfs:comment>
        <rdfs:label>Has Window Start Time</rdfs:label>
        <owl:equivalentProperty>http://dbpedia.org/property/starts</owl:equivalentProperty>
        <owl:equivalentProperty>https://schema.org/startTime</owl:equivalentProperty>
