    </owl:Class>
    


    <!-- http://semanticsalah.com/ontology/Asar -->

    <owl:Class rdf:about="http://semanticsalah.com/ontology/Asar">
        <rdfs:subClassOf rdf:resource="http://semanticsalah.com/ontology/ObligatorySalah"/>
    </owl:Class>
    


    <!-- http://semanticsalah.com/ontology/CompleteRakah -->

    <owl:Class rdf:about="http://semanticsalah.com/ontology/CompleteRakah">
        <rdfs:subClassOf rdf:resource="http://semanticsalah.com/ontology/Rakah"/>
        <rdfs:label xml:lang="en">Complete Rakah</rdfs:label>
    </owl:Class>
    


    <!-- http://semanticsalah.com/ontology/CompletedFourUnitRakah1 -->

    <owl:Class rdf:about="http://semanticsalah.com/ontology/CompletedFourUnitRakah1">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://semanticsalah.com/ontology/belongsToUnit"/>
                                <owl:someValuesFrom rdf:resource="http://semanticsalah.com/ontology/FourSalahUnit"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://semanticsalah.com/ontology/hasRakahNumber"/>
                                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:hasValue>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://semanticsalah.com/ontology/containsPosture"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://semanticsalah.com/ontology/QayamPosture"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://semanticsalah.com/ontology/followedBy"/>
                                        <owl:someValuesFrom>
                                            <owl:Class>
                                                <owl:intersectionOf rdf:parseType="Collection">
                                                    <rdf:Description rdf:about="http://semanticsalah.com/ontology/RukuPosture"/>
                                                    <owl:Restriction>
                                                        <owl:onProperty rdf:resource="http://semanticsalah.com/ontology/followedBy"/>
                                                        <owl:someValuesFrom>
                                                            <owl:Class>
                                                                <owl:intersectionOf rdf:parseType="Collection">
                                                                    <rdf:Description rdf:about="http://semanticsalah.com/ontology/QoumPosture"/>
                                                                    <owl:Restriction>
                                                                        <owl:onProperty rdf:resource="http://semanticsalah.com/ontology/followedBy"/>
                                                                        <owl:someValuesFrom>
                                                                            <owl:Class>
                                                                                <owl:intersectionOf rdf:parseType="Collection">
                                                                                    <rdf:Description rdf:about="http://semanticsalah.com/ontology/SajdaPosture"/>
                                                                                    <owl:Restriction>
                                                                                        <owl:onProperty rdf:resource="http://semanticsalah.com/ontology/followedBy"/>
                                                                                        <owl:someValuesFrom>
                                                                                            <owl:Class>
                                                                                                <owl:intersectionOf rdf:parseType="Collection">
                                                                                                    <rdf:Description rdf:about="http://semanticsalah.com/ontology/JalsaPosture"/>
                                                                                                    <owl:Restriction>
                                                                                                        <owl:onProperty rdf:resource="http://semanticsalah.com/ontology/followedBy"/>
                                                                                                        <owl:someValuesFrom rdf:resource="http://semanticsalah.com/ontology/SajdaPosture"/>
                                                                                                    </owl:Restriction>
                                                                                                </owl:intersectionOf>
                                                                                            </owl:Class>
                                                                                        </owl:someValuesFrom>
                                                                                    </owl:Restriction>
                                                                                </owl:intersectionOf>
                                                                            </owl:Class>
                                                                        </owl:someValuesFrom>
                                                                    </owl:Restriction>
                                                                </owl:intersectionOf>
                                                            </owl:Class>
                                                        </owl:someValuesFrom>
                                                    </owl:Restriction>
                                                </owl:intersectionOf>
                                            </owl:Class>
                                        </owl:someValuesFrom>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://semanticsalah.com/ontology/CompleteRakah"/>
    </owl:Class>
    


    <!-- http://semanticsalah.com/ontology/CompletedFourUnitRakah2 -->

    <owl:Class rdf:about="http://semanticsalah.com/ontology/CompletedFourUnitRakah2">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://semanticsalah.com/ontology/belongsToUnit"/>
                        <owl:someValuesFrom rdf:resource="http://semanticsalah.com/ontology/FourSalahUnit"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://semanticsalah.com/ontology/containsPosture"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://semanticsalah.com/ontology/QayamPosture"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://semanticsalah.com/ontology/followedBy"/>
                                        <owl:someValuesFrom>
                                            <owl:Class>
                                                <owl:intersectionOf rdf:parseType="Collection">
                                                    <rdf:Description rdf:about="http://semanticsalah.com/ontology/RukuPosture"/>
                                                    <owl:Restriction>
                                                        <owl:onProperty rdf:resource="http://semanticsalah.com/ontology/followedBy"/>
                                                        <owl:someValuesFrom>
                                                            <owl:Class>
                                                                <owl:intersectionOf rdf:parseType="Collection">
                                                                    <rdf:Description rdf:about="http://semanticsalah.com/ontology/QoumPosture"/>
                                                                    <owl:Restriction>
                                                                        <owl:onProperty rdf:resource="http://semanticsalah.com/ontology/followedBy"/>
                                                                        <owl:someValuesFrom>
                                                                            <owl:Class>
                                                                                <owl:intersectionOf rdf:parseType="Collection">
                                                                                    <rdf:Description rdf:about="http://semanticsalah.com/ontology/SajdaPosture"/>
                                                                                    <owl:Restriction>
                                                                                        <owl:onProperty rdf:resource="http://semanticsalah.com/ontology/followedBy"/>
                                                                                        <owl:someValuesFrom>
