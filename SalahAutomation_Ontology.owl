        <rdfs:label>Performed In Location</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://semanticsalah.com/ontology/postureEndsAtTime -->

    <owl:DatatypeProperty rdf:about="http://semanticsalah.com/ontology/postureEndsAtTime">
        <rdfs:domain rdf:resource="http://semanticsalah.com/ontology/Postures"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
